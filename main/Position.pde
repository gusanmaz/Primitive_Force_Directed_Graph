public class Position{
  private float x;
  private float y;
  
  public Position(float _x, float _y){
    x = _x;
    y = _y;
  }
  
  public void setPosition(float _x, float _y){
    x = _x;
    y = _y;
  }
  
  public float getX(){
    return x;
  }
  
  public float getY(){
    return y;
  }
  
}
