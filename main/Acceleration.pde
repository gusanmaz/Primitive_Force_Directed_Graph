public class Acceleration{
  private float x;
  private float y;
  
  public Acceleration(float _x, float _y){
    x = _x;
    y = _y;
  }
  
  public float getX(){
    return x;
  }
  
  public float getY(){
    return y;
  }
  
  public void set(float _x, float _y){
    x = _x;
    y = _y;
  }
   
}
